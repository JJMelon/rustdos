#![no_std]
#![no_main]

use core::panic::PanicInfo;

mod vga_buffer;

// Taken from https://os.phil-opp.com/minimal-rust-kernel/#building-our-kernel
#[no_mangle]
pub extern "C" fn _start() -> ! {
    println!("Hello World{}", "!");
}


#[panic_handler]
fn panic(info: &PanicInfo) -> ! {
    println!("{}", info);

    loop{}
}
